#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/frontend/node_modules/.pnpm/@graphql-codegen+cli@6.1.0_@parcel+watcher@2.5.1_@types+node@22.19.2_crossws@0.4.1_srvx_81c76c1b8cb7c4e8bd537261df6d802a/node_modules/@graphql-codegen/cli/esm/node_modules:/workspace/frontend/node_modules/.pnpm/@graphql-codegen+cli@6.1.0_@parcel+watcher@2.5.1_@types+node@22.19.2_crossws@0.4.1_srvx_81c76c1b8cb7c4e8bd537261df6d802a/node_modules/@graphql-codegen/cli/node_modules:/workspace/frontend/node_modules/.pnpm/@graphql-codegen+cli@6.1.0_@parcel+watcher@2.5.1_@types+node@22.19.2_crossws@0.4.1_srvx_81c76c1b8cb7c4e8bd537261df6d802a/node_modules/@graphql-codegen/node_modules:/workspace/frontend/node_modules/.pnpm/@graphql-codegen+cli@6.1.0_@parcel+watcher@2.5.1_@types+node@22.19.2_crossws@0.4.1_srvx_81c76c1b8cb7c4e8bd537261df6d802a/node_modules:/workspace/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/frontend/node_modules/.pnpm/@graphql-codegen+cli@6.1.0_@parcel+watcher@2.5.1_@types+node@22.19.2_crossws@0.4.1_srvx_81c76c1b8cb7c4e8bd537261df6d802a/node_modules/@graphql-codegen/cli/esm/node_modules:/workspace/frontend/node_modules/.pnpm/@graphql-codegen+cli@6.1.0_@parcel+watcher@2.5.1_@types+node@22.19.2_crossws@0.4.1_srvx_81c76c1b8cb7c4e8bd537261df6d802a/node_modules/@graphql-codegen/cli/node_modules:/workspace/frontend/node_modules/.pnpm/@graphql-codegen+cli@6.1.0_@parcel+watcher@2.5.1_@types+node@22.19.2_crossws@0.4.1_srvx_81c76c1b8cb7c4e8bd537261df6d802a/node_modules/@graphql-codegen/node_modules:/workspace/frontend/node_modules/.pnpm/@graphql-codegen+cli@6.1.0_@parcel+watcher@2.5.1_@types+node@22.19.2_crossws@0.4.1_srvx_81c76c1b8cb7c4e8bd537261df6d802a/node_modules:/workspace/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@graphql-codegen/cli/esm/bin.js" "$@"
else
  exec node  "$basedir/../@graphql-codegen/cli/esm/bin.js" "$@"
fi
