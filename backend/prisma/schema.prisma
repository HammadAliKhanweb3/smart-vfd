  // Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

  generator client {
    provider        = "prisma-client"
    output          = "../src/generated/prisma"
    moduleFormat    = "cjs"
  }


  datasource db {
    provider = "postgresql"
  }

  model SensorsData {
   id Int @default(autoincrement()) @id
   inputVoltage Float
   inputCurrent Float?
   voltagePhaseOne Float?
   currentPhaseOne Float?
   voltagePhaseTwo Float?
   currentPhaseTwo Float?
   voltagePhaseThree Float?
   currentPhaseThree Float?
   vfdTemperature Float?
   motorTemperature Float?
   motorTorque Float?
   motorSpeed Float?
   createdAt DateTime @default(now())
   
  }